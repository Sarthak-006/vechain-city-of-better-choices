{
    "version": 2,
    "name": "city-of-better-choices",
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaDuration": 1800
            }
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ],
    "functions": {
        "api/index.py": {
            "maxDuration": 1800
        }
    }
}
